<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.openrat.de/template ../template.xsd">
	<form>
		<group title="message:global_prop">
			<part class="line">
				<part class="label">
					<label for="name">
						<text text="GLOBAL_name"></text>
					</label>
				</part>
				<part class="input">
					<input name="name" class="name,focus"></input>
				</part>
			</part>

			<part class="line">
				<part class="label">
					<label for="description">
						<text text="GLOBAL_description"></text>
					</label>
				</part>
				<part class="input">
					<inputarea name="description" class="description"></inputarea>
				</part>
			</part>
		</group>

	</form>
</output>