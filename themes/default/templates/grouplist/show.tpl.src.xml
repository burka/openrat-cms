<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.openrat.de/template ../template.xsd">
	<table>
		<row class="headline">
			<column>
				<text key="name"></text>
			</column>
		</row>
		<list list="el" extract="true">
			<row class="data">
				<column id="var:id" name="var:name" action="group">>
					<image file="icon_group"></image>
					<text var="name"></text>
				</column>
			</row>
		</list>
		<row class="data">
			<column colspan="2">
				<link type="view" subaction="add">
					<image icon="add"/>
					<text text="new"/>
				</link>
			</column>
		</row>
	</table>
</output>