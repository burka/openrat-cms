<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.openrat.de/template ../template.xsd">
	<table>
		<row>
			<column class="help">
				<text key="GLOBAL_NAME"></text>
			</column>
			<column class="help">
				<text key="GLOBAL_LASTCHANGE"></text>
			</column>
		</row>
		<list list="result" extract="true">
			<row class="data">
				<column>
					<link url="var:url" target="cms_main">
						<image type="var:type"></image>
						<text var="name" title="var:desc"></text>
					</link>
				</column>
				<column>
					<date date="var:lastchange_date"></date>
				</column>
			</row>
		</list>
	</table>
</output>