<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.openrat.de/template ../template.xsd">
	<form method="post">
		<part class="line">
			<part class="label">
				<label for="timezone_offset">
					<text key="timezone" />
				</label>
			</part>
			<part class="input">
				<selectbox name="timezone_offset" list="timezone_list"
					addempty="true"></selectbox>
			</part>
		</part>
		<part class="line">
			<part class="label">
				<label for="">
					<text key="language" />
				</label>
			</part>
			<part class="input">
				<selectbox name="language" list="language_list" addempty="true"></selectbox>
			</part>
		</part>
	</form>
</output>