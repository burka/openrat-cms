<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.openrat.de/template ../../../../../../template-engine/components/template.xsd">
	<header></header>
	<form label="message:global_save">

		<hidden name="languageid" />
		<part class="line">
			<part class="label">
				<label for="name" key="global_name" />
			</part>
			<part class="input">
				<input name="name" class="name,focus"></input>
			</part>
		</part>
		<part class="line">
			<part class="label">
				<label for="filename" key="global_filename" />
			</part>
			<part class="input">
				<input name="filename" class="filename" maxlength="150" />
			</part>
		</part>
		<part class="line">
			<part class="label">
				<label for="description" key="global_description">
				</label>
			</part>
			<part class="input">
				<inputarea name="description" class="description"></inputarea>
			</part>
		</part>
	</form>
</output>