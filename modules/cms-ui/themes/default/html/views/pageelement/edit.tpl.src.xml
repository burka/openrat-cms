<output xmlns="http://www.openrat.de/template" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.openrat.de/template ../../../../../../template-engine/components/template.xsd">

	<table>
		<row class="headline">
			<column header="true">
				<text key="language" />
			</column>
			<column header="true">
				<text key="value" />
			</column>
		</row>

		<list list="languages" extract="true">
			<row class="data">
				<column>
					<text var="languagename" />
				</column>
				<column class="clickable">
					<link subaction="value" var1="languageid" value1="var:languageid" type="edit" action="pageelement">
						<text var="value" maxlength="140" escape="true" />
					</link>
				</column>
				<column>
				</column>
			</row>
		</list>
	</table>
</output>